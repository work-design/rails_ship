<tr id="tr_<%= model.id %>" data-controller="show" data-action="mouseenter->show#show mouseleave->show#hide">
  <%= yield %>
  <td>
    <div data-show-target="item" class="visibility-hidden">
      <%= link_to({ controller: 'box_sells', box_proxy_sell_id: model.id }, aria: { label: t('ship.admin.box_sells.index.title') }, class: 'button is-small is-rounded is-light') do %>
        <i class="fa-solid fa-pencil"></i>
      <% end %>
    </div>
  </td>
</tr>
