<p>
  <span class="tag is-success"><%= model.from_station&.name %></span>
</p>
<p>
  <span><%= model.address.content %></span>
  <span class="tag is-danger"><%= model.station&.name %></span>
</p>
<p><%= model.box&.code %></p>
<p class="is-size-7">
  <time data-controller="time" class="has-text-info-dark"><%= model.boxed_in_at&.to_fs(:iso8601) %></time>
  <i class="fa-solid fa-right-long has-text-grey px-1"></i>
  <time data-controller="time" class="has-text-warning-dark"><%= model.boxed_out_at&.to_fs(:iso8601) %></time>
</p>
<p><%= model.expected_on %></p>
<p class="has-text-grey is-size-7"><time data-controller="time"><%= model.created_at.to_fs(:iso8601) %></time></p>
