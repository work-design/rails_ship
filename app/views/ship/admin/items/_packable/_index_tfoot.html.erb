<tr>
  <th>
    <div data-controller="check" data-check-name-value="xx" class="is-flex is-align-items-center">
      <%= check_box_tag 'xxd', 'xx', false, id: nil, data: { action: 'check#toggleAll' } %>
      <%= form_with url: { action: 'package' }, method: :post, class: 'button_to ml-2' do |f| %>
        <%= f.hidden_field :ids, data: { check_target: 'ids' } %>
        <%= f.hidden_field :address_id, value: params[:address_id] %>
        <%= button_tag data: { action: 'check#doSubmit' }, aria: { label: t('.package') }, class: 'button is-small is-dark' do %>
          <i class="fa-regular fa-hand-holding-usd"></i>
        <% end %>
      <% end %>
    </div>
  </th>
  <th colspan="7"></th>
</tr>
