<th>
  <div data-controller="check" data-check-name-value="xx" class="flex align-items-center">
    <%= check_box_tag 'xxd', 'xx', false, id: nil, data: { action: 'check#toggleAll' } %>
    <%= form_with url: { action: 'unloaded_create' }, method: :post, class: 'button_to ml-2' do |f| %>
      <%= f.hidden_field :ids, data: { check_target: 'ids' } %>
      <%= button_tag data: { action: 'check#doSubmit' }, aria: { label: t('.title') }, class: 'button is-small is-dark' do %>
        <i class="fa-solid fa-person-dolly-empty has-text-danger"></i>
      <% end %>
    <% end %>
  </div>
</th>
<th><%= Ship::Package.human_attribute_name(:station_id) %></th>
<th><%= Ship::Package.human_attribute_name(:address_id) %></th>
<th><%= Ship::Package.human_attribute_name(:state) %></th>
<th>
  <p><%= Ship::Package.human_attribute_name(:box_id) %></p>
  <p class="has-text-info-dark"><%= Ship::Package.human_attribute_name(:boxed_in_at) %></p>
  <p class="has-text-warning-dark"><%= Ship::Package.human_attribute_name(:boxed_out_at) %></p>
</th>
<th><%= Ship::Package.human_attribute_name(:created_at) %></th>
