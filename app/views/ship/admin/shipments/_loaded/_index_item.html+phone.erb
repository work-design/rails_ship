<p>
  <span><%= model.address.content %></span>
  <span class="tag is-success"><%= model.station&.name %></span>
</p>
<div>
  <p><%= model.box&.code %></p>
  <p class="text-info-dark"><time data-controller="time"><%= model.boxed_in_at&.to_fs(:iso8601) %></time></p>
  <p class="text-warning-dark"><time data-controller="time"><%= model.boxed_out_at&.to_fs(:iso8601) %></time></p>
</div>
<p class="size-7 text-grey">
  <time data-controller="time"><%= model.created_at.to_fs(:iso8601) %></time>
</p>
