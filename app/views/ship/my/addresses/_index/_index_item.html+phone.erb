<h6 class="weui-media-box__title">
  <span><%= model.contact_person %></span>
  <span><%= model.tel %></span>
</h6>
<div class="weui-media-box__desc size-7">
  <span><%= model.area.full_name %></span>
  <span><%= model.detail %></span>
  <% if model.principal %>
  <span class="tag">
    <i class="fa-solid fa-circle-check text-success"></i>
    <span class="pl-1">默认地址</span>
  </span>
  <% end %>
</div>
